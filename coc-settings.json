{
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "vue",
    "ruby",
    "reason",
    "scss",
    "css",
    "ocaml",
    "haskell"
  ],
  "eslint.filetypes": [
    "vue",
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact"
  ],
  "eslint.options": {
    "useEslintrc": true
  },
  "eslint.autoFixOnSave": true,
  "diagnostic.errorSign": ">>",
  "npm.binPath": "yarn",
  "languageserver": {
    "reason": {
      "command": "~/Apps/reason-lsp/reason-language-server",
      "filetypes": ["reason"]
    },
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    },
    "ocaml": {
      "command": "ocaml-language-server",
      "args": ["--stdio"],
      "filetypes": ["ocaml"]
    }
  }
}
